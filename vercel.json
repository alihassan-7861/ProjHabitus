{
  "version": 2,
  "builds": [
    {
      "src": "Habitus/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Habitus/wsgi.py"
    }
  ],
  "buildCommand": "pip install -r requirements.txt && python3.9 manage.py collectstatic --noinput"
}
